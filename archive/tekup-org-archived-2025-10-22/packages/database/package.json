  "devDependencies": {
    "prisma": "^6.16.0",
    "typescript": "^5.6.3",
    "@types/node": "^20.14.10",
    "tsx": "^4.11.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12"
  },
  "exports": {
