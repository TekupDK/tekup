{
  "name": "@tekup/testing",
  "version": "0.1.0",
  "private": false,
  "publishConfig": {
    "access": "restricted"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest -c jest.config.cjs",
    "test:ci": "jest -c jest.config.cjs --ci --maxWorkers=50%",
    "coverage": "jest -c jest.config.cjs --coverage",
    "flakes": "node ../../scripts/detect-flakes.cjs --repeats 5",
    "typecheck": "tsc --noEmit",
    "demo": "npx tsx demo.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@nestjs/testing": "^10.4.20",
    "@prisma/client": "^6.16.0",
    "artillery": "^2.0.0",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "playwright": "^1.40.0",
    "redis": "^4.6.0",
    "supertest": "^7.1.4",
    "testcontainers": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/supertest": "^2.0.16",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@tekup/config": "workspace:*",
    "@tekup/shared": "workspace:*"
  }
}
