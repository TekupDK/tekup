{
  "name": "@tekup/flow-api",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "pnpm --filter @tekup/config build && ts-node -r tsconfig-paths/register src/main.ts",
    "build": "pnpm --filter @tekup/config build && nest build",
    "start": "node dist/main.js",
    "test": "jest -c jest.config.cjs",
    "test:ci": "jest -c jest.config.cjs --ci --maxWorkers=50%",
    "coverage": "jest -c jest.config.cjs --coverage",
    "flakes": "node ../../scripts/detect-flakes.cjs --repeats 5",
    "migrate:test": "cross-env DATABASE_URL=%DATABASE_URL% prisma migrate deploy",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "build:exe": "nest build && pkg dist/main.js --targets node20-win-x64 --output dist/tekup-api.exe --config pkg.config.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^6.16.0",
    "@tekup/config": "workspace:*",
    "@tekup/shared": "workspace:*",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.3.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "isomorphic-dompurify": "^2.26.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "prisma": "^6.16.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
