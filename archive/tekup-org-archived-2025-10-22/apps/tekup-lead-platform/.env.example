# TekUp Lead Platform Environment Variables

# Database
PX_DATABASE_URL="postgresql://user:password@localhost:5432/tekup_leads"

# Redis Cache
REDIS_URL="redis://localhost:6379"

# Server Configuration
PORT=3003
NODE_ENV=development

# Google Workspace Integration (Rendetalje)
GOOGLE_SERVICE_ACCOUNT_PATH="/path/to/service-account.json"
GOOGLE_CALENDAR_ID="primary"

# Billy Billing Integration (Rendetalje)
BILLY_API_KEY="your_billy_api_key_here"

# Email Configuration
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="info@rendetalje.dk"
SMTP_PASS="your_app_password"

# Webhook URLs
FLOW_API_URL="http://localhost:3000"
FLOW_WEB_URL="http://localhost:3001"

# Security
JWT_SECRET="your_jwt_secret_here"
API_KEY_SALT="your_api_key_salt_here"
