{
  "name": "@tekup/lead-platform",
  "version": "0.1.0",
  "private": true,
  "description": "TekUp Lead Platform - Advanced multi-tenant lead management and qualification system",
  "main": "dist/main.js",
  "scripts": {
    "dev": "pnpm --filter @tekup/config build && ts-node -r tsconfig-paths/register src/main.ts",
    "build": "pnpm --filter @tekup/config build && nest build",
    "start": "node dist/main.js",
    "test": "jest --passWithNoTests",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^6.16.0",
    "@tekup/api-client": "workspace:*",
    "@tekup/config": "workspace:*",
    "@tekup/shared": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "googleapis": "^144.0.0",
    "ioredis": "^5.3.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.13",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
