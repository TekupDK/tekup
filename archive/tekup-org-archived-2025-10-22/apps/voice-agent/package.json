    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "jsdom": "^24.1.3",
    "postcss": "^8.5.6",
