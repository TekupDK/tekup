# AgentScope Enhanced Backend Environment Configuration

# API Keys (Required)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Jarvis Foundation Model Configuration
JARVIS_MODEL_NAME=jarvis-foundation-1.0
JARVIS_MODEL_TYPE=foundation
JARVIS_MAX_TOKENS=32768
JARVIS_TEMPERATURE=0.7
JARVIS_DANISH_SUPPORT=true
JARVIS_MULTI_AGENT_OPTIMIZATION=true
JARVIS_REAL_TIME_STEERING=true

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8001
SERVER_RELOAD=true
SERVER_LOG_LEVEL=info

# CORS Configuration
CORS_ORIGINS=["http://localhost:3005", "http://localhost:3000", "https://jarvis.tekup.org"]
CORS_ALLOW_CREDENTIALS=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Database Configuration (for future use)
# DATABASE_URL=postgresql://user:password@localhost:5432/jarvis_agentscope
# REDIS_URL=redis://localhost:6379/0

# Monitoring (for future use)
# PROMETHEUS_ENABLED=true
# PROMETHEUS_PORT=9090
# GRAFANA_ENABLED=true

# Security
SECRET_KEY=your_secret_key_here_change_in_production
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Performance Tuning
MAX_CONCURRENT_TASKS=10
MAX_STEERING_SESSIONS=50
WEBSOCKET_TIMEOUT=300

# Development flags
DEBUG=true
ENABLE_API_DOCS=true
