# Test Environment Configuration for Microsoft Agent Framework

# Server Configuration
PORT=3000
LOG_LEVEL=info
NODE_ENV=development

# Database Configuration (Required)
DATABASE_URL="postgresql://test:test@localhost:5432/renos_test"

# Google Configuration (Required)
RUN_MODE=dry-run
GOOGLE_PROJECT_ID=test-project
GOOGLE_CLIENT_EMAIL=test@test.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nTEST\n-----END PRIVATE KEY-----\n"
GOOGLE_IMPERSONATED_USER=test@test.com
GOOGLE_CALENDAR_ID=primary
DEFAULT_EMAIL_FROM=test@test.com
ORGANISATION_NAME=Rendetalje.dk

# LLM Configuration (Optional for testing)
LLM_PROVIDER=heuristic
OPENAI_API_KEY=
GEMINI_KEY=
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1:8b

# Microsoft Agent Framework Configuration
ENABLE_MICROSOFT_ORCHESTRATION=false
ENABLE_MICROSOFT_THREADS=true
ENABLE_MICROSOFT_TELEMETRY=true
ENABLE_MICROSOFT_PLUGINS=false
ENABLE_MICROSOFT_HYBRID=true

# Test Configuration
SMOKETEST_EMAIL_TO=test@test.com
SMOKETEST_CALENDAR_ID=primary
SMOKETEST_TIMEZONE=Europe/Copenhagen

# CORS Configuration
FRONTEND_URL=http://localhost:5173
CORS_ORIGIN=http://localhost:5173

# Authentication (Disabled for testing)
ENABLE_AUTH=false