{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "env": {
        "MEMORY_FILE_PATH": "C:\\Users\\empir\\.mcp-shared\\memory.json"
      },
      "disabled": false,
      "alwaysAllow": []
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "disabled": false,
      "alwaysAllow": []
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "C:/Users/empir/Tekup"],
      "disabled": false,
      "alwaysAllow": ["read_file", "list_directory"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "disabled": false,
      "alwaysAllow": []
    },
    "knowledge-mcp": {
      "command": "docker",
      "args": ["exec", "-i", "tekup-knowledge-mcp", "node", "dist/index.js"],
      "disabled": false,
      "alwaysAllow": [],
      "description": "Docker: Documentation search"
    },
    "code-intelligence-mcp": {
      "command": "docker",
      "args": ["exec", "-i", "tekup-code-intelligence-mcp", "node", "dist/index.js"],
      "disabled": false,
      "alwaysAllow": [],
      "description": "Docker: Code search & analysis"
    },
    "database-mcp": {
      "command": "docker",
      "args": ["exec", "-i", "tekup-database-mcp", "node", "dist/index.js"],
      "disabled": false,
      "alwaysAllow": [],
      "description": "Docker: Supabase queries"
    },
    "billy-mcp": {
      "command": "docker",
      "args": ["exec", "-i", "tekup-billy-mcp", "node", "dist/http-server.js"],
      "disabled": false,
      "alwaysAllow": [],
      "description": "Docker: Billy.dk integration"
    },
    "calendar-mcp": {
      "command": "docker",
      "args": ["exec", "-i", "tekup-calendar-mcp", "node", "dist/index.js"],
      "disabled": false,
      "alwaysAllow": [],
      "description": "Docker: Google Calendar"
    }
  },
  "globalShortcuts": {}
}
