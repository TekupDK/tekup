{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "C:\\Users\\empir\\.mcp-shared\\memory.json"
      },
      "disabled": false,
      "alwaysAllow": []
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabled": false,
      "alwaysAllow": []
    },
    "tekup-billy": {
      "command": "node",
      "args": [
        "C:/Users/empir/Tekup/apps/production/tekup-billy/dist/index.js"
      ],
      "env": {
        "BILLY_API_KEY": "${BILLY_API_KEY}",
        "BILLY_ORGANIZATION_ID": "${BILLY_ORGANIZATION_ID}",
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_ANON_KEY": "${SUPABASE_ANON_KEY}",
        "NODE_ENV": "${NODE_ENV}",
        "LOG_LEVEL": "${LOG_LEVEL}"
      },
      "disabled": false,
      "alwaysAllow": []
    },
    "tekup-vault": {
      "command": "node",
      "args": [
        "C:/Users/empir/Tekup/apps/production/tekup-vault/apps/mcp/dist/index.js"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "disabled": false,
      "alwaysAllow": []
    },
    "calendar-mcp": {
      "command": "node",
      "args": [
        "C:/Users/empir/Tekup/apps/rendetalje/services/calendar-mcp/dist/index.js"
      ],
      "disabled": false,
      "alwaysAllow": []
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:/Users/empir/Tekup"
      ],
      "disabled": false,
      "alwaysAllow": ["read_file", "list_directory"]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "disabled": false,
      "alwaysAllow": []
    }
  },
  "globalShortcuts": {}
}
