{
  "name": "tekup",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.17.0",
  "scripts": {
    "build": "pnpm -r --if-present build",
    "dev": "pnpm -r --parallel --if-present dev",
    "test": "pnpm -r --if-present test",
    "lint": "pnpm -r --if-present lint",
    "clean": "pnpm -r --if-present clean",
    "markdown:lint": "markdownlint-cli2 \"**/*.md\" \"#node_modules\" \"#archive\" \"#.git\"",
    "markdown:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\" \"#archive\" \"#.git\"",
    "markdown:check": "markdownlint-cli2 --config .markdownlint.json \"**/*.md\"",
    "commitlint": "commitlint --edit",
    "prepare": "node -e \"try { require('husky').install() } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e }\""
  },
  "dependencies": {
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "husky": "^9.0.0",
    "markdownlint-cli2": "^0.16.0",
    "typescript": "^5.9.3"
  }
}
