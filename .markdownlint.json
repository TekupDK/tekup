{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/v0.38.0/schema/markdownlint-config-schema.json",
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD010": {
    "code_blocks": true,
    "spaces_per_tab": 2
  },
  "MD012": {
    "maximum": 2
  },
  "MD013": false,
  "MD024": {
    "siblings_only": true
  },
  "MD025": {
    "front_matter_title": ""
  },
  "MD026": {
    "punctuation": ".,;:!"
  },
  "MD029": {
    "style": "ordered"
  },
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD031": false,
  "MD033": {
    "allowed_elements": [
      "details",
      "summary",
      "br",
      "img",
      "a",
      "kbd",
      "sub",
      "sup",
      "div",
      "span",
      "table",
      "tr",
      "td",
      "th",
      "thead",
      "tbody"
    ]
  },
  "MD034": true,
  "MD035": {
    "style": "---"
  },
  "MD036": false,
  "MD040": false,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD048": {
    "style": "backtick"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  },
  "MD051": false,
  "MD052": {
    "shortcut_syntax": false
  },
  "MD053": {
    "ignored_definitions": ["//"]
  }
}
