{
  "name": "@tekup/google-mcp",
  "version": "1.0.0",
  "description": "Google Workspace MCP Server for Tekup - Gmail and Calendar integration via Model Context Protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/http-server.ts",
    "start:http": "tsx src/http-server.ts",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http-server.ts",
    "test": "tsx tests/test-integration.ts",
    "test:watch": "tsx --watch tests/test-integration.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "google",
    "gmail",
    "calendar",
    "workspace",
    "ai",
    "tekup"
  ],
  "author": "Tekup",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "google-auth-library": "^9.0.0",
    "googleapis": "^144.0.0",
    "helmet": "^8.1.0",
    "winston": "^3.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.21",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
