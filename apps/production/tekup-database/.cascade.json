{
  "$schema": "https://cascade.dev/schema.json",
  "version": "1.0",
  "project": {
    "name": "tekup-database",
    "type": "database",
    "language": "typescript"
  },
  "commands": {
    "allowlist": [
      "pnpm install",
      "pnpm db:generate",
      "pnpm db:push",
      "pnpm db:studio",
      "pnpm db:migrate",
      "pnpm db:health",
      "pnpm db:backup",
      "pnpm db:restore",
      "pnpm db:seed",
      "pnpm test",
      "pnpm build",
      "docker-compose up -d",
      "docker-compose down",
      "docker-compose ps",
      "docker ps",
      "docker logs",
      "git status",
      "git add",
      "git commit",
      "git log",
      "git diff",
      "psql",
      "prisma generate",
      "prisma migrate",
      "prisma db push",
      "prisma studio"
    ],
    "autoApprove": {
      "readOnly": true,
      "database": true,
      "git": false
    }
  },
  "autoRun": {
    "safeCommands": [
      "pnpm db:health",
      "docker ps",
      "git status",
      "pnpm test"
    ]
  }
}
