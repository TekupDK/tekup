# RenOS Calendar MCP Environment Variables

# Database
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
POSTGRES_PASSWORD=your_postgres_password_here

# Google Calendar
GOOGLE_CALENDAR_CREDENTIALS={"type":"service_account","project_id":"your_project_id"}

# Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Billy.dk
BILLY_API_KEY=your_billy_api_key
BILLY_COMPANY_ID=your_billy_company_id

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Redis
REDIS_URL=redis://redis:6379

# App URLs
MCP_SERVER_URL=http://localhost:3001
DASHBOARD_URL=http://localhost:3006
CHATBOT_URL=http://localhost:3005
