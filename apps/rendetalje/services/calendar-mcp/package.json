{
  "name": "@renos/calendar-mcp",
  "version": "0.1.0",
  "description": "RenOS Calendar Intelligence MCP - Intelligent booking validation, overtime tracking, and customer memory for Rendetalje.dk",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http-server.ts",
    "inspect": "npx @modelcontextprotocol/inspector dist/index.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:verbose": "jest --config jest.config.cjs --verbose",
    "test:integration": "jest --config jest.config.cjs --testPathPattern=integration",
    "test:unit": "jest --config jest.config.cjs --testPathPattern=unit",
    "snapshot:perf": "node scripts/snapshot-perf.js",
    "snapshot:perf:ps": "powershell -File scripts/snapshot-perf.ps1",
    "docker:build": "docker build -t renos-calendar-mcp .",
    "docker:run": "docker run --env-file .env -p 3001:3001 renos-calendar-mcp",
    "check:ports": "node scripts/check-ports.js",
    "docker:up": "npm run check:ports && docker-compose up",
    "docker:up:detached": "npm run check:ports && docker-compose up -d",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "mcp",
    "calendar",
    "booking",
    "intelligence",
    "rendetalje",
    "renos",
    "ai"
  ],
  "author": "Tekup - Jonas Abde",
  "license": "MIT",
  "dependencies": {
    "@langchain/openai": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tekup/database": "file:../../../production/tekup-database",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.0.0",
    "dotenv": "^16.3.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "googleapis": "^131.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "langchain": "^1.0.1",
    "opossum": "^8.1.4",
    "rate-limit-redis": "^4.2.0",
    "twilio": "^5.0.0",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.21",
    "@types/opossum": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
