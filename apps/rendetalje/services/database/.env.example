# Supabase Configuration for RendetaljeOS
# Copy this file to .env and fill in your actual values

# Supabase Project Settings
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Connection
DATABASE_URL=postgresql://postgres:your-password@db.your-project-id.supabase.co:5432/postgres

# JWT Settings
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=24h

# Storage Configuration
SUPABASE_STORAGE_URL=https://your-project-id.supabase.co/storage/v1

# Real-time Configuration
SUPABASE_REALTIME_URL=wss://your-project-id.supabase.co/realtime/v1

# External Service Integrations
TEKUP_BILLY_URL=https://tekup-billy.onrender.com
TEKUP_BILLY_API_KEY=your-billy-api-key

TEKUPVAULT_URL=https://tekupvault.onrender.com
TEKUPVAULT_API_KEY=your-vault-api-key

AI_FRIDAY_URL=https://tekup-chat.onrender.com
AI_FRIDAY_API_KEY=your-friday-api-key

RENOS_CALENDAR_URL=https://renos-calendar-mcp.onrender.com
RENOS_CALENDAR_API_KEY=your-calendar-api-key

# Google Services (for Calendar and Gmail integration)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# SMS Configuration (for notifications)
TWILIO_ACCOUNT_SID=your-twilio-sid
TWILIO_AUTH_TOKEN=your-twilio-token
TWILIO_PHONE_NUMBER=+1234567890

# File Upload Configuration
MAX_FILE_SIZE=10485760  # 10MB in bytes
ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp
ALLOWED_DOCUMENT_TYPES=application/pdf,text/plain

# Security Configuration
ENCRYPTION_KEY=your-32-character-encryption-key
CORS_ORIGIN=http://localhost:3000,https://rendetaljeos.dk

# Development Configuration
NODE_ENV=development
PORT=3001
LOG_LEVEL=debug

# Production Configuration (uncomment for production)
# NODE_ENV=production
# PORT=3001
# LOG_LEVEL=info
# CORS_ORIGIN=https://rendetaljeos.dk