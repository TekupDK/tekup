# RendetaljeOS Backend - Docker Environment
# Used when running in Docker containers

# Database
DATABASE_URL=postgresql://rendetalje_user:rendetalje_password@postgres:5432/rendetalje

# Redis
REDIS_URL=redis://redis:6379

# Server
NODE_ENV=development
PORT=3001

# Security
JWT_SECRET=development-jwt-secret-change-in-production
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100
