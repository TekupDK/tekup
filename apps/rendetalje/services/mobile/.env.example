# RendetaljeOS Mobile App - Environment Variables
#
# Copy this file to .env.local and update with your values

# Backend API URL (set your computer's local IP)
EXPO_PUBLIC_API_URL=http://192.168.1.100:3001/api

# Supabase (if using)
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Feature Flags
EXPO_PUBLIC_ENABLE_AI_FRIDAY=true
EXPO_PUBLIC_ENABLE_BIOMETRIC=true
EXPO_PUBLIC_ENABLE_OFFLINE=true

# Development
EXPO_PUBLIC_ENVIRONMENT=development
