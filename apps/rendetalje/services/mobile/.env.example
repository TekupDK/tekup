# RendetaljeOS Mobile App - Environment Variables Template
#
# Copy this file to .env.local and fill in your actual values
# DO NOT commit .env.local to version control

# ============================================
# Backend API Configuration
# ============================================
# The base URL for your RendetaljeOS backend API
# Development: http://localhost:3001/api or http://YOUR_IP:3001/api
# Staging: https://staging-api.rendetalje.dk/api
# Production: https://api.rendetalje.dk/api
EXPO_PUBLIC_API_URL=http://192.168.1.100:3001/api

# ============================================
# Supabase Configuration (if using Supabase)
# ============================================
# Your Supabase project URL
# Example: https://abcdefghijklmnop.supabase.co
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url

# Your Supabase anonymous/public key
# Example: eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# ============================================
# Google Maps Configuration (Optional)
# ============================================
# Google Maps API key for geocoding and map features
# Get your API key from: https://console.cloud.google.com/
# Enable: Maps SDK for Android, Maps SDK for iOS, Geocoding API
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=

# ============================================
# Push Notifications (Optional)
# ============================================
# Expo Push Notification Token
# Leave empty to use Expo's push notification service
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=

# ============================================
# Feature Flags
# ============================================
# Enable/disable features for testing
EXPO_PUBLIC_ENABLE_AI_FRIDAY=true
EXPO_PUBLIC_ENABLE_BIOMETRIC=true
EXPO_PUBLIC_ENABLE_OFFLINE=true
EXPO_PUBLIC_ENABLE_GPS_TRACKING=true

# ============================================
# App Configuration
# ============================================
# App environment: development, staging, production
EXPO_PUBLIC_ENVIRONMENT=development

# Enable debug mode (shows additional logs)
EXPO_PUBLIC_DEBUG_MODE=true

# API timeout in milliseconds (default: 30000)
EXPO_PUBLIC_API_TIMEOUT=30000

# ============================================
# Analytics & Monitoring (Optional)
# ============================================
# Sentry DSN for error tracking
EXPO_PUBLIC_SENTRY_DSN=

# Google Analytics ID
EXPO_PUBLIC_GA_TRACKING_ID=

# ============================================
# Notes
# ============================================
# - All variables prefixed with EXPO_PUBLIC_ are exposed to the client
# - Never commit sensitive keys to version control
# - Use different values for development, staging, and production
# - Restart Expo server after changing environment variables
# - For physical devices, use your computer's local network IP instead of localhost
